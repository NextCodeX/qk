arr = [1, false, true, 3.14, "changlie"]


println("------------------continue test: ")
for a=0;a<5;a++ {
    if a==3 {
        continue
    }
    println("current value:", arr[a])
}


println("------------------break test: ")
for a=0;a<5;a++ {
    if a==3 {
        break
    }
    println("current value:", arr[a])
}