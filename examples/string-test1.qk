s = "hello world    "
println("size", s.size())

printf("trim: -%v-\n", s.trim())
println("split", s.split(" "))
println("replace", s.replace("he", "wc"))
println("upper", s.upper())
println("title", s.title())
println("upperFirst", s.upperFirst())
println("sub", s.sub(2), ", ", s.sub(3, 7))
println("contain", s.contain("wo"), s.contain("jk"))