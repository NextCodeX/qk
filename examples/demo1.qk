db = sqlite("db2")

db.exec(`
create table if not exists stu (
    id integer primary key,
    num int,
    icon blob,
    name nvarchar(50),
    sex int(1),
    age int,
    tall real,
    birth varchar(20),
    tel char(11),
    province nvarchar(50),
    city nvarchar(50),
    depart nvarchar(50),
    degree  nvarchar(50),
    salary real,
    remark text
)
`)

db.exec(`
create table if not exists emps (
    id integer primary key,
    name varchar(50)
)
`)


rows = db.rows("SELECT * FROM sqlite_schema")

rows.pr()

